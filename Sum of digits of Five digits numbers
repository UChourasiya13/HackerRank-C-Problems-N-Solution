/* Given a five digit integer, print the sum of its digits. */

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n , sum=0 , rem ;
    scanf("%d", &n);
    for(int i = 0 ; i < 5 ;i++){
        rem = n % 10 ;
        sum = sum + rem ;
        n = n/10 ;
    }
    printf("%d" , sum);
    return 0;
}
